{% extends 'template.html.twig' %}
{% form_theme form 'form_div_layout.html.twig' %}

{% block stylesheet %}
<style>
	.gauto-find-area {
		position: relative;
		top: -100px;
		z-index: 6;
	}

	.find-form form p {
		margin: 15px 0;
	}

	.find-form p input {
		width: 100%;
		border: 2px solid #ebebeb;
		padding: 5px 10px;
		height: 45px;
		color: #111;
		-webkit-transition: all 0.4s ease 0s;
		transition: all 0.4s ease 0s;
	}

	.find-form p.error input {
		border: 2px solid #ee3333;
	}

	.search-widget {
		display: flex;
		/*z-index: 999;*/
		box-shadow: 3px 0 29px rgba(0, 0, 0, 0.26);
	}

	.search-widget .image {
		flex: 1.25;
		background: #fff url(/public/image/image.png) no-repeat scroll 0 0;
		display: flex;
		background-position: center;
		background-size: cover;
		min-height: 100px;
		text-align: center;
		padding: 30px;
	}

	.search-widget .image h3 {
		color: #fff;
		font-size: 33px;
		font-weight: 600;
		text-transform: uppercase;
		align-self: center;
		margin: 0;
		width: 100%;
	}

	.search-widget .form {
		flex: 3;
		padding: 30px;
		background: white;
	}

	.policy-block {
		border-bottom: none;
	}

	@media(min-width: 768px) {
		.policy-block {
			margin-top: 0px !important;
		}
	}

	@media(max-width: 767px) {
		.search-widget {
			flex-direction: column;
			z-index: initial;
		}
		.gauto-find-area {
			top: 20px;
		}
		.search-widget .image {
			min-height: 220px;
		}
		.policy-block .inner {
			grid-template-columns: 100%;
		}
	}
</style>
{% endblock %}

